<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/textViewNama"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nama Mahasiswa"
        android:textSize="20sp"
        android:textStyle="bold"
        android:layout_marginStart="20dp"
        android:layout_marginVertical="25dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textViewNIM"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="NIM Mahasiswa"
        android:textSize="16sp"
        android:layout_marginStart="20dp"
        android:layout_marginVertical="25dp"
        app:layout_constraintTop_toBottomOf="@id/textViewNama"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textViewKey"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginVertical="25dp"
        android:text="Key"
        android:visibility="gone"
        app:layout_constraintStart_toEndOf="@+id/textViewNIM"
        app:layout_constraintTop_toBottomOf="@id/textViewNama" />

    <TextView
        android:id="@+id/textViewTempatLahir"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Tempat Lahir Mahasiswa"
        android:textSize="16sp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="25dp"
        android:paddingBottom="25dp"
        app:layout_constraintTop_toBottomOf="@id/textViewNIM"
        app:layout_constraintStart_toStartOf="parent"/>

</androidx.constraintlayout.widget.ConstraintLayout>